{
  "name": "ngx-duality",
  "version": "1.0.2",
  "description": "Angular library seed",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular4",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "library",
    "ng",
    "ngx",
    "styles",
    "directives",
    "components",
    "services",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Danny908/Duality"
  },
  "author": {
    "name": "Daniel Torres",
    "email": "danny908t@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Danny908/Duality/issues"
  },
  "homepage": "",
  "scripts": {
    "build": "npm run build:esm && npm run build:umd && npm run sass && gulp copy:core && gulp copy:manifest && gulp copy:README",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run lint && npm run test && npm run build && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp docs",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage --disablePrivateOrInternalSupport",
    "explorer": "source-map-explorer ./dist/index.umd.js",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "lint": "npm run tslint 'src/**/*.ts'",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "tslint": "tslint './src/**/*.ts'",
    "sass": "gulp sass-external-files"
  },
  "dependencies": {
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "^2.8.2",
    "@types/webpack-env": "^1.13.2",
    "codecov": "^2.3.1",
    "codelyzer": "^3.2.2",
    "karma-webpack": "^2.0.6",
    "source-map-explorer": "^1.5.0",
    "typescript": "^2.5.3",
    "zone.js": "^0.8.18"
  },
  "peerDependencies": {
    "@angular/common": "^5.0.5",
    "@angular/core": "^5.0.5",
    "@angular/compiler": "^5.0.5",
    "@angular/compiler-cli": "^5.0.5",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@angular/common": "^5.0.5",
    "@angular/compiler": "^5.0.5",
    "@angular/compiler-cli": "^5.0.5",
    "@angular/core": "^5.0.5",
    "@angular/platform-browser": "^5.0.5",
    "@angular/platform-browser-dynamic": "^5.0.5",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "^2.8.2",
    "@types/karma": "^1.7.2",
    "@types/node": "^8.0.55",
    "@types/webpack": "^3.8.1",
    "@types/webpack-env": "^1.13.2",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.1",
    "codecov": "^3.0.0",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "gh-pages": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "gulp-sass": "^3.1.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.6",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "rimraf": "2.6.2",
    "rxjs": "^5.5.4",
    "sass-loader": "^6.0.6",
    "source-map-explorer": "^1.5.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-angular-externals": "^1.0.2",
    "webpack-rxjs-externals": "^1.1.0",
    "zone.js": "^0.8.18"
  }
}
